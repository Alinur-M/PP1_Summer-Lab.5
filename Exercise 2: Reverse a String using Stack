#include <iostream>
#include <stack>
#include <string>
using namespace std;

int main() {
    string input;
    stack<char> charStack;

    cout << "Enter a string: ";
    getline(std::cin, input);

    for (char c : input) {
        charStack.push(c);
    }

    cout << "Reversed string: ";
    
    while (!charStack.empty()) {
        cout << charStack.top();
        charStack.pop();
    }

    return 0;
}
